            "pass": "applications/catalog-server"
        }
    },

    "applications": {
        "catalog-server": {
            "type": "python 3.7",
            "path": "/www",
            "module": "app.main",
            "callable": "app",
            "environment": {
              "MONGO_URI": "mongodb://nobody:seekrit@db:27017",
              "CATSHTM_DIR": "/data/catsHTM"
            }
        }
    }
}
